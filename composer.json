{
    "name": "jackardios/laravel-query-wizard",
    "description": "Laravel Query Wizard",
    "keywords": [
        "jackardios",
        "laravel-query-builder",
        "laravel-query-wizard",
        "laravel",
        "query",
        "wizard",
        "builder",
        "json",
        "api",
        "json api",
        "query builder"
    ],
    "homepage": "https://github.com/jackardios/laravel-query-wizard",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/jackardios/laravel-query-wizard/issues",
        "source": "https://github.com/jackardios/laravel-query-wizard"
    },
    "authors": [
        {
            "name": "Salavat Salakhutdinov",
            "email": "salavat.salahutdinov@gmail.com",
            "homepage": "https://github.com/jackardios"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^v10.0"
    },
    "require-dev": {
        "ext-json": "*",
        "larastan/larastan": "^2.0",
        "laravel/legacy-factories": "^v1.4.0",
        "orchestra/testbench": "^v8.21.0",
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "Jackardios\\QueryWizard\\": "src"
        },
        "files": [
            "./src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Jackardios\\QueryWizard\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox",
        "test-coverage": "XDEBUG_MODE=coverage vendor/bin/phpunit --testdox --coverage-text",
        "phpstan": "vendor/bin/phpstan analyse --memory-limit=512M"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jackardios\\QueryWizard\\QueryWizardServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
